{
  "name": "Inhaltsstoff.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run server && npm run statics-server && npm run statics && webpack-dev-server --config client/webpack.config.babel.js --content-base build/ --hot --inline",
    "prod": "npm run server && npm run statics-server && npm run statics && NODE_ENV=prod webpack --config client/webpack.config.babel.js",
    "server": "rm -rf build/data && babel-node server/index.js --presets es2015",
    "statics": "mkdir -p build/ && rm -vf build/*.{svg,js,gz,png,html} && cp client/index.html build/index.html && cp client/robots.txt build/ && cp client/favicon.png build/",
    "statics-server": "mkdir -p build/ && rm -rvf build/statics && cp -R content/statics build/statics",
    "test": "npm run prod"
  },
  "license": "ISC",
  "keywords": [
    "angular2",
    "es6",
    "webpack",
    "gulp"
  ],
  "description": "Film- und Buchrezensionen seit 2011",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "angular2-moment": "0.8.0",
    "es6-promise": "3.1.2",
    "es6-shim": "0.35.0",
    "foundation-sites": "6.2.1",
    "fs-extra": "0.30.0",
    "lodash": "4.12.0",
    "normalize.css": "4.1.1",
    "object-path": "0.9.2",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "utterson": "1.0.4",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-cli": "6.9.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-preset-angular2": "0.0.2",
    "babel-preset-es2015": "6.14.0",
    "css-loader": "0.23.1",
    "es6-promise-loader": "1.0.2",
    "eslint": "3.0.0",
