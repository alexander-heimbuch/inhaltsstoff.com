{
  "name": "Inhaltsstoff.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run generator && npm run statics-generator && npm run statics && webpack-dev-server --config client/webpack.config.babel.js --content-base build/ --quiet --hot --inline",
    "prod": "npm run generator && npm run statics-generator && npm run statics && NODE_ENV=prod webpack --config client/webpack.config.babel.js",
    "generator": "rm -rf build/data && babel-node generator/index.js --presets es2015",
    "statics": "mkdir -p build/ && rm -vf build/*.{svg,js,gz,png,html} && cp client/index.html build/index.html && cp client/robots.txt build/ && cp client/favicon.png build/",
    "statics-generator": "mkdir -p build/ && rm -rvf build/statics && cp -R content/statics build/statics",
    "test": "npm run prod"
  },
  "license": "ISC",
  "keywords": [
    "angular2",
    "es6",
    "webpack",
    "gulp"
  ],
  "description": "Film- und Buchrezensionen seit 2011",
  "dependencies": {
    "@angular/common": "5.2.3",
    "@angular/compiler": "5.2.3",
    "@angular/core": "5.2.3",
    "@angular/http": "5.2.3",
    "@angular/platform-browser": "5.2.3",
    "@angular/platform-browser-dynamic": "5.2.3",
    "@angular/router": "5.2.3",
    "angular2-moment": "1.8.0",
    "es6-promise": "4.2.4",
    "es6-shim": "0.35.3",
    "foundation-sites": "6.4.3",
    "fs-extra": "5.0.0",
    "lodash": "4.17.4",
    "normalize.css": "7.0.0",
    "object-path": "0.11.4",
    "reflect-metadata": "0.1.12",
    "rxjs": "5.5.6",
    "systemjs": "0.20.19",
    "utterson": "1.0.5",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-preset-angular2": "0.0.2",
    "babel-preset-es2015": "6.24.1",
    "css-loader": "0.28.9",
    "es6-promise-loader": "1.0.2",
    "eslint": "4.16.0",
    "file-loader": "1.1.6",
    "html-loader": "0.5.5",
    "json-loader": "0.5.7",
