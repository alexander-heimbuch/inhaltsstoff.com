    "style-loader": "0.20.1",
    "surge": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "2.11.1",
    "webpack-livereload-plugin": "1.0.0"
  }
